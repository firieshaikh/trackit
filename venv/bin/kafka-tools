#!/home/feroz/track_it/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'pykafka','console_scripts','kafka-tools'
__requires__ = 'pykafka'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pykafka', 'console_scripts', 'kafka-tools')()
    )
